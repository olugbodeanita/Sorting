#include <iostream>

template<class T>
void swap(T& x, T& y){
  T temp = x;
  x = y;
  y = temp;
}



template<class T>
void print(T data, int n){
  for(int i = 0; i < n; i++){
    std::cout << data[i] << std::endl;
  }
}

